    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.20",
    "@types/express-jwt": "^7.4.2",
    "@types/helmet": "^4.0.0",
    "@types/jsonwebtoken": "^9.0.4",
    "@types/node": "^20.12.8",
    "ts-node-dev": "^2.0.0",
    "tslib": "^2.6.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
